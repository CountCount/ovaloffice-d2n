			FROM dvoo_stat_zombies x 
			INNER JOIN dvoo_towns t ON t.id = x.tid  
			WHERE x.day > 0  AND (t.stamp - ((t.day - 1) * 86400)) > 1311262200 AND (t.stamp - ((t.day - 1) * 86400)) < 1321398300 ORDER BY x.z DESC LIMIT 1


